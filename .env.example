# ========================================
# APPLICATION CONFIGURATION
# ========================================
SERVER_PORT=8090

# ========================================
# MYSQL CONFIGURATION
# ========================================
MYSQL_ROOT_PASSWORD=your_mysql_password
MYSQL_DATABASE=your_database_name
MYSQL_USERNAME=your_mysql_username
MYSQL_PORT=3306

# ========================================
# PHPMYADMIN CONFIGURATION
# ========================================
PHPMYADMIN_PORT=8080

# ========================================
# MONGODB CONFIGURATION
# ========================================
MONGO_ROOT_USERNAME=your_mongo_username
MONGO_ROOT_PASSWORD=your_mongo_password
MONGO_PORT=27017
MONGO_DATABASE=your_mongo_database

# ========================================
# MONGO EXPRESS CONFIGURATION
# ========================================
MONGO_EXPRESS_PORT=8081
MONGO_EXPRESS_USERNAME=your_mongo_express_username
MONGO_EXPRESS_PASSWORD=your_mongo_express_password

# ========================================
# MINIO CONFIGURATION
# ========================================
MINIO_ROOT_USER=your_minio_user
MINIO_ROOT_PASSWORD=your_minio_password
MINIO_API_PORT=9000
MINIO_CONSOLE_PORT=9001

# ========================================
# JPA/HIBERNATE CONFIGURATION
# ========================================
JPA_DDL_AUTO=update
JPA_SHOW_SQL=true
JPA_FORMAT_SQL=true
JPA_USE_SQL_COMMENTS=true

# ========================================
# LOGGING CONFIGURATION
# ========================================
LOG_LEVEL_JDBC=DEBUG
LOG_LEVEL_HIBERNATE_SQL=DEBUG
LOG_LEVEL_HIBERNATE_BINDER=TRACE

# ========================================
# ACTUATOR CONFIGURATION
# ========================================
ACTUATOR_ENDPOINTS=health,info,metrics
ACTUATOR_HEALTH_DETAILS=always

